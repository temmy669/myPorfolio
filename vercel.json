{
    "builds": [
        {
            "src": "myPortfolio/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        },
        {
            "src": "staticfiles/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "myPortfolio/wsgi.py"
        }
    ]
}
